{
  "name": "Fasterize indicator",
  "manifest_version": 2,
  "description": "an indicator when a page is fasterized in the address bar.",
  "version": "0.0.1",
  "permissions": ["tabs"],
  "background": { "scripts": ["indicator.js"] },
  "web_accessible_resources" : [
    "script.js"
  ],
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content.js"],
    "run_at": "document_idle"
  }],
  "page_action": {
    "default_title": "Fasterized",
  },
  "icons": {
    "128": "icon-extension.png"
  }
}
