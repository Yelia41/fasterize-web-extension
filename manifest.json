{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "Fasterize Status",
  "version": "0.1",
  "manifest_version": 2,
  "description": "See the status of the request.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webNavigation",
	"webRequest",
	"tabs",
	"http://*/*",
	"https://*/*"
   ]
}
